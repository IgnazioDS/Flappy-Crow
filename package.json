{
  "name": "flappy-bird-ex",
  "private": true,
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:ios": "tsc && vite build --base=./",
    "cap:sync:ios": "cap sync ios",
    "cap:open:ios": "cap open ios",
    "ios:smoke": "npm run build:ios && npm run cap:sync:ios",
    "preview": "vite preview",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.1",
    "@capacitor/ios": "^8.0.1",
    "@playwright/test": "^1.55.0",
    "@types/node": "^25.0.9",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@capacitor/core": "^8.0.1",
    "cordova-plugin-purchase": "^13.12.1",
    "phaser": "^3.90.0"
  },
  "overrides": {
    "tar": "^7.5.6"
  }
}
